{
  "extends": [ "airbnb/base" ],
  "plugins": [ "import" ],
  "settings": {
    "import/resolver": "node"
  },
  "rules": {
    "no-shadow": [
      0
    ],
    "no-use-before-define": [
      0
    ],
    "class-methods-use-this": [
      0
    ],
    "global-require": [
      0
    ]
    },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  }
}
